{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "***",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "StripeSettings": {
    "SecretKey": "***",
    "WebHookKey": "***",
    "Headers": "Stripe-Signature"
  },
  "ConnectionStrings": {
    "DefaultConnection": "***"
  },
  "AllowedHosts": "*"
}
